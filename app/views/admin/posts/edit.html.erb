<h4>投稿編集</h4>
<div class="container">
<div class='row'>
<div class="mx-auto">
<%= form_with model: @post, url: admin_post_path(@post.id) do |f| %>


<div class="col-2">
<p>タイトル
</div>
<div class="col-8">
 <%= f.text_field :title %></p>
</div>

<div class="col-2">
<p>説明
</div>
<div class="col-8">
  <%= f.text_area :body %></p>
</div>


<%= f.submit '変更を保存', class: "btn btn-success" %>
<% end %>
</div>
</div>
</div>


