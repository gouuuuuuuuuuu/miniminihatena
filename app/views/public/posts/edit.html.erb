<h4>投稿編集</h4>
<div class="container">
<div class="mx-auto">
<%= form_with model: @post, url: public_post_path(@post.id) do |f| %>
<div class='row'>

<div class="col-6 mb-4">
<p>タイトル
</div>
<div class="col-6">
 <%= f.text_field :title %></p>
</div>

<div class="col-6 mb-4">
<p>説明
</div>
<div class="col-6">
  <%= f.text_area :body %></p>
</div>

<div class="col-3">
</div>
<div class="col-9">
<%= f.submit '変更を保存', class: "btn btn-success" %>
</div>
<% end %>

