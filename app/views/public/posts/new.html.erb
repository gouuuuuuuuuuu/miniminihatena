<div class="mx-auto">
<%= form_with model: @post, url: public_posts_path, local: true do |form| %>

<p><%= form.label :title %><br>
<%= form.text_field :title %>
</p>

<p><%= form.label :body %><br>
<%= form.text_area :body %>
</p>

<p><%= form.submit '投稿する', class: "btn btn-success" %>
</p>

<% end %>
</div>